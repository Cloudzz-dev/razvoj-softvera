# OpenAI
OPENAI_API_KEY=your_api_key_here

# NextAuth
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_random_secret_here

# OAuth Providers
GITHUB_ID=your_github_id
GITHUB_SECRET=your_github_secret
GOOGLE_CLIENT_ID=your_google_id
GOOGLE_CLIENT_SECRET=your_google_secret

# Database (Supabase Subscription)
# Connect to Transaction Pooler
DATABASE_URL="postgres://[user]:[password]@[host]:6543/[db]?pgbouncer=true"
# Connect to Session Pooler / Direct
DIRECT_URL="postgres://[user]:[password]@[host]:5432/[db]"

# Supabase Keys
NEXT_PUBLIC_SUPABASE_URL="https://your-project.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-anon-key"
SUPABASE_SERVICE_ROLE_KEY="your-service-role-key"

# Redis (optional - for production rate limiting)
REDIS_URL=redis://localhost:6379
